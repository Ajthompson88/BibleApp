{
    "compilerOptions": {
      "target": "es2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "./dist",
      "strict": true,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {
        "@models/*": ["models/*"]
      },
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules"]
  }
  